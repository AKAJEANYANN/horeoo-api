{
  "name": "messageServeur",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mysql": {
      "table": "messageserveur"
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "message": {
      "type": "string",
      "default": "",
      "mysql": {
        "dataType": "TEXT"
      }
    },
    "dateMessage": {
      "type": "date"
    },
    "objetMessage": {
      "type": "string",
      "default": ""
    },
    "vueMessage": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "provider": {
      "type": "belongsTo",
      "model": "provider",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "postMessage"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "postMessageHeberge"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "notifpaiement"
    }
  ],
  "methods": {}
}
