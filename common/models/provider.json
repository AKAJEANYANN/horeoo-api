{
  "name": "provider",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "nomCompletProvider": {
      "type": "string",
      "default": ""
    },
    "CreatedDateProvider": {
      "type": "date",
      "defaultFn": "now"
    },
    "etatProvider": {
      "type": "string",
      "default": ""
    },
    "approuveProvider": {
      "type": "boolean",
      "default": false
    },
    "approval_datetime": {
      "type": "date",
      "defaultFn": "now"
    },
    "rectoCniProvider": {
      "type": "string",
      "default": ""
    },
    "versoCniProvider": {
      "type": "string",
      "default": ""
    },
    "phoneProvider": {
      "type": "string",
      "default": ""
    },
    "imageProvider": {
      "type": "string",
      "default": ""
    },
    "activeProvider": {
      "type": "boolean",
      "default": false
    },
    "activeDateProvider": {
      "type": "date",
      "defaultFn": "now"
    },
    "desactiveDateProvider": {
      "type": "date",
      "defaultFn": "now"
    },
    "device_fcm_token": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "appAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "hebergements": {
      "type": "hasMany",
      "model": "hebergement",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "messageServeurs": {
      "type": "hasMany",
      "model": "messageServeur",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "commercial": {
      "type": "belongsTo",
      "model": "commercial",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "phonenumber"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "number"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "affiche"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "afficheDemandeApprouve"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "afficheApprouve"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "afficheDesapprouve"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "mapfilter"
    }
  ],
  "methods": {}
}
