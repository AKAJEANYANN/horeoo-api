{
  "name": "bot",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "botForUser": {
      "type": "boolean"
    },
    "activeBot": {
      "type": "boolean"
    },
    "botMessage": {
      "type": "string"
    },
    "etatReservation": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "reservations": {
      "type": "hasMany",
      "model": "reservation",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
